{
  "version": 2,
  "name": "crypto-swap-storybook",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "buildCommand": "pnpm build-storybook",
        "distDir": "storybook-static",
        "nodeVersion": "22.x",
        "installCommand": "pnpm install"
      }
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}